package Ejercicio2Meteorologo;

import java.util.ArrayList;

public abstract class Prediccion {
	int cantmaximaRegistros;
	int valoraComparar;
	
	protected Prediccion(int c, int v) {
		cantmaximaRegistros=c;
		valoraComparar=v;
	}
	
	public abstract boolean predicelluvia(EstacionMeteorologica e);

	@Override
	public String toString() {
		return "Prediccion [cantmaximaRegistros=" + cantmaximaRegistros + ", valoraComparar=" + valoraComparar + "]";
	}
	

	
	
}
