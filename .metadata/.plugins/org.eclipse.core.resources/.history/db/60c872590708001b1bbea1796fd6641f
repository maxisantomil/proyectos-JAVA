
public class CriterioHastaTalDuracion extends Criterio {
	private int duracion;
	
	public CriterioHastaTalDuracion(int duracion) {
		this.duracion=duracion;
	}
	
	@Override
	public boolean cumple(Pelicula p) {
		if (p.getDuracion()< duracion) {
			return true;
		}
		return false;
	}

}
