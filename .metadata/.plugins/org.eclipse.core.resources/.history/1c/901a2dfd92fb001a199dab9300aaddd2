package Ejercicio1Contrato;

public class CriterioSueldo extends Criterio {
	int sueldo;
	
	public CriterioSueldo(int s) {
		sueldo=s;
	}
	@Override
	public boolean acepta(Contrato c) {
		// TODO Auto-generated method stub
		if (c.getSueldo()<sueldo) {
			return true;
		}
		
		return false;
	}

}
