package Ejercicio4Contribuyentes;

import java.util.ArrayList;

public class Ciudad {
	String nombre;
	int cantHabitantes;
	ArrayList<Contribuyente>contribuyentes; 
	
	
	
	public double montoRecaudado () {
		double sumaMonto;
		for (Contribuyente x:contribuyentes) {
			sumaMonto+=x.getMonto();
		}
		return sumaMonto;
	}
}
