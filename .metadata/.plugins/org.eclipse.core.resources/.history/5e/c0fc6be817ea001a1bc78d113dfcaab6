
public class Rectangulo {
  PuntoGeometrico vertice1;
  PuntoGeometrico vertice2;
  PuntoGeometrico vertice3;
  PuntoGeometrico vertice4;
   
  public Rectangulo(PuntoGeometrico a,PuntoGeometrico b,PuntoGeometrico c,PuntoGeometrico d) {
	  vertice1= a;
	  vertice2=b;
	  vertice3=c;
	  vertice4=d;
  }
}

